{
  "expo": {
    "name": "Reciper",
    "description": "Unlimited recipes for daily inspiration",
    "slug": "Reciper",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "4.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "package": "com.usfslk.reciperapp",
      "permissions": [
        "READ_PHONE_STATE"
      ],
      "versionCode": 2
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}